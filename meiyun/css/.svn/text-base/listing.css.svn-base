body{
	background: #fff;
	font-size: 0.32rem;
}
.head2{
	background: #fafafa;
}
.filter_box{
	width:100%;
	font-size: 0;
	background: #fff;
	border-bottom:1px solid #ddd;
	position: relative;
}
.filter_box  li{
	display: inline-block;
	font-size:0.32rem;
	height:0.8rem;
	line-height: 0.8rem;
	border-right:1px solid #ddd;
	width:33.3%;
	text-align: center;
	position: relative;
}
.nav_box{
	position: absolute;
	background: #fff;
	width: 100%;
}
.nav_box ul{
	display: none;
}
.nav_box li{
	text-indent: 0.4rem;
    line-height: 0.8rem;
    border-bottom: 1px solid #ebebeb;
}
.nav_box li.on{
	background: url(../images/check2.png) no-repeat 9rem center;
	background-size: 0.4rem;
}
.name .stylist{
	font-size:0.48rem;
	font-weight: 600;
}
.name .aft{
	font-size:0.15rem;
	vertical-align: super;
	border-left:1px solid #ddd;
	padding-left:0.2rem;
}
.mask{
	position: fixed;
	top:0;
	left:0;
	right:0;
	bottom:0;
	background:rgba(0,0,0,0.5);
	display: none;
}
.head_nav{
	display: inline-block;
	border-radius: 0.2rem;
	font-size:0.5rem;
}
.head_nav span{

}
.head_right{
	position: absolute;
	right:0;
	top:0;
}
.head_right .wallet{
	background: url(../images/wallet2.png) no-repeat center;
	background-size: 0.64rem;
	padding:0.4rem;
}
.filter_box  li:nth-child(3){
	border-right:0px solid #ddd;
}
.list_box{
	overflow: hidden;
	border-bottom:1px solid #ddd;
}
.list_box .use_box{
	float: left;
	width:2.5rem;
	display: inline-block;
	text-align: right;
	height:2rem;
	padding-top:0.4rem;
}
.list_box .use_box img{
	width: 2rem;
	height:2rem;
	border-radius: 9999px;
	border:0;
	outline: none;
}
.skill .name{
	line-height: 0.6rem;
	overflow: hidden;
}
.mor{
	background: url(../images/mor2.png) no-repeat center;
	background-size: 0.64rem;
	padding:0.4rem;
}
.name>span{
	float: right;
	color:#1bb7ac;
	border:1px solid #1bb7ac;
	padding:0.1rem 0.2rem;
}
.list_box .skill{
	margin-left:3rem;
	padding: 0.4rem 0.2rem 0 0;
}
.revise{
	background: url(../images/revise.png) no-repeat center;
	background-size: 0.51rem;
	padding:0.26rem;
}
.skill>p{

}
.pic{
	font-size: 0;
}
.pic img{
	width:2rem;
	margin:0 0.3rem 0.3rem 0;
	height:2rem;
	background: red;
}
.level{
	padding:0.2rem 0;
	color:orange;
}
.level span{
	color:#333;
	margin-left:0.2rem;
}
.forte{
	color:#999;
	font-size:0.33rem;
	margin-bottom:0.2rem;

}
.pic img:nth-child(3n){
	margin:0 0 0.3rem 0 ;
}
.address{
	color:#999;
	padding-bottom:0.4rem;
	padding-left:0.4rem;
	font-size:0.33rem;
	background: url(../images/address.png) no-repeat left 0.05rem;
	background-size: 0.32rem 0.37rem;

}
.address>span{
	float: right;
	
}
.worth{
	font-size:0.33rem;
	background: url(../images/worth.png) no-repeat left center;
	background-size: 0.33rem;
	padding-left:0.4rem;
}
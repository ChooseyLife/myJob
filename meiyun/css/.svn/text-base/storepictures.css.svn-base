.head_top>span{
	position: absolute;
	top:0.2rem;
	left:0.2rem;
	z-index:10;
	color:#fff;
	background: url(../images/back2.png) no-repeat center;
    background-size: 0.64rem;
    background-color: rgba(0,0,0,.3);
    width: 0.8rem;
    height: 0.8rem;
    display: inline-block;
    border-radius: 9999px;


}
.head_top>a{
	position: absolute;
	right: 0.4rem;
	top:0.4rem;
	z-index:10;
	color:#fff;
	background: url(../images/news.png) no-repeat center;
    background-size: 0.64rem;
    background-color: rgba(0,0,0,.3);
    width: 0.8rem;
    height: 0.8rem;
    display: inline-block;
    border-radius: 9999px;
}
.nav_box{
	height:7rem;
	position: relative;

}
.swiper-wrapper{
	width:100%;
	height:100%;
	background: red;
}
.swiper-container,.swiper-container2{
	height:100%;
}
.swiper-slide img{
	width:100%;
	height: 100%;
}
.datum_box{
	background: #fff;
}
.touch_box{
	padding:0.4rem;
	background: #fff;
}
.touch_box .phone{
	float: right;
	background: url(../images/phone2.png) no-repeat center;
	background-size: 0.64rem;
	padding:0.4rem;

}
.touch_box>div{
	margin-right: 1.5rem;
}
.address{
	background: url(../images/go.png) no-repeat 9.5rem center;
	background-size: 0.32rem 0.43rem;
	padding:0.4rem;
	color:#333;
}
.datum{
	padding:0.4rem;
	margin-left:0.4rem;
	border-left:2px solid #ddd;
}
.datum .mor{
	display: inline-block;
	margin-top:0.6rem;
	color:#0cc;
}
.pick_box .designers{
	padding:0.4rem;
	font-weight: 600;
	font-size:0.47rem;
}
.designers i{
	font-weight: normal;
	font-size:0.24rem;
	margin-left:0.2rem;
}
.designers_box{
	background: #fff;
	padding-left:0.4rem;
}
.designers_list{
	overflow: hidden;
	padding:0.4rem 0;
}
.designers_list .left{
	float: left;
	display: inline-block;
	border-radius:999px;
	overflow: hidden;
	height: 1rem;
	width: 1rem;
	line-height: 1rem;
	text-align: center;

}
.name_box .right{
	color:#0cc;
}
.name_box .style{
	padding-right:1.2rem;
}
.designers_list .left img{
	border-radius:999px;
	width: 100%;
	height: 100%;
}
.designers_list:nth-last-child(1){
	padding-bottom:0;
	
}
.designers_list:nth-last-child(1) .name_box{
	border:0;
}
.name_box{
	overflow: hidden;
	border-bottom:1px solid #ddd;
	padding:0.1rem 0.4rem 0.4rem 0.1rem;
}
.name_box .right{
	float: right;
	margin-top:0.3rem;
}
.name_box .name{
	font-size:0.4rem;
	margin-bottom:0.2rem;
	font-weight: 600;
}
.mor_box{
	color:#0cc;
	border-top:1px solid #ddd;
	border-bottom:1px solid #ddd;
	background: #fff;
	line-height: 1rem;
	text-align: center;
}
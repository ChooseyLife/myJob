body{
	background: #eee;
}
.head_box{
	height:2.8rem;
	padding: 0.4rem;
	margin-bottom:0.4rem;
	background: url(../images/orders-bg.png) no-repeat;
	background-size: cover;
	color:#fff;
}

.wire{
	display: block;
	width: 100%;
	height:0.2rem;
	background: url(../images/wire.png) -0.2rem;
	background-size: cover;
}
.contact_box{
	color:#999;
	border-left:0;
	border-right:0;
	padding:0.2rem;
	background: #fff;
}
.contact_box .name{
	font-weight: 600;
	margin-top:0.4rem;
	margin-bottom:0.5rem;
	font-size:0.4rem;
	color:#000;
}
.place_box{
	position: relative;
}
.place_box .title{
	float: left;
}
.place_box .place{
	display: block;
	margin-left:1.8rem;
	margin-right:1.4rem;
	text-align: justify;
}
.place_box .map{
	/* background: red; */
	padding:0.4rem;
	position: absolute;
	right: 0;
	bottom:0;
}
.contact_box .name a{
	float: right;
}
.contact_box p{
	margin-bottom:0.2rem;
}
.time a{
	margin-left:0.2rem;
	color:orange;
}
.mechanic_box{
	padding:0.4rem;
	background: #fff;
	margin-bottom:0.4rem;
}
.mechanic_box .name{
	margin-bottom:0.8rem;

}
.staff{
	font-weight: 600;
	background: url(../images/go.png) no-repeat right center;
	background-size: 0.32rem 0.43rem;
	padding-right:0.5rem;

}
.mechanic_box .name .contact{
	float: right;
	background: url(../images/contact.png) no-repeat left center;
	background-size: 0.64rem;
	padding-left:0.8rem;
}
.item{
	background: url(../images/go.png) no-repeat right center;
	background-size: 0.32rem 0.43rem;
}
.item a{
	display: inline-block;
	width: 100%;
	height: 100%;
}
.mechanic_box .item img{
	width:2rem;
	height: 2rem;
	vertical-align: middle;
}
.mechanic_box .item span{
	vertical-align: middle;
	margin-left:0.4rem;
}
.explain_title{
	padding: 0.2rem 0.4rem;
	color:#999;
}
.explain_title a{
	float: right;
	color:#999;
}
.phone{
	display: block;
	color:#0cc;
	text-align: center;
	margin-top:0.6rem;
}

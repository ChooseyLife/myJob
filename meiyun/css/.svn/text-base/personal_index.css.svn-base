body{
    background: #dddddd;
}
.head-content{
    background:url("http://bos.pgzs.com/rbpiczy/Wallpaper/2012/8/21/ef6c9e596feb4fefabecb3084fd7b8c2-9.jpg")no-repeat;
}
.head-Img,.personal-name,.integral{
    display: block;
    margin:0 auto;
    text-align: center;
}
.personal-name,.integral{
    width:6rem;
    padding:.05rem;
    color:#fff;
}

.mune img{
    width:0.64rem;
}

.head-Img{
    width: 1.2rem;
    height: 1.2rem;
    border-radius: 99px;
    background: url(../images/zlfxs.png)no-repeat;
    background-size:100% 100%;
}

.project_box{
    background-color: #fff;
    overflow: hidden;
}
.project_box li{
    float: left;
    padding-top: .8rem;
    padding-bottom: .2rem;
    width: 25%;
    text-align: center;
    background-size:.93rem;
}
.order{
    background:#fff url("../images/personImg/order.png")no-repeat center top;
}
.art{
    background:#fff url("../images/personImg/art.png")no-repeat center top;
}
.opus{
    background:#fff url("../images/personImg/opus.png")no-repeat center top;
}
.wallet{
    background:#fff url("../images/personImg/wallet.png")no-repeat center top;
}

.tit-message{
    background:url("../images/personImg/mess.png")no-repeat .1rem center;
}
.tit-invi{
    background:url("../images/personImg/gi.png")no-repeat .1rem center;
}
.tit-code{
    background:url("../images/personImg/code.png")no-repeat .1rem center;
}
.tit-coupon{
    background:url("../images/personImg/coupon.png")no-repeat .1rem center;
}
.tit-sign{
    background:url("../images/personImg/integral.png")no-repeat .1rem center;
}
.tit-advise{
    background:url("../images/personImg/advi.png")no-repeat .1rem center;
}
.tit-about{
    background:url("../images/personImg/about.png")no-repeat .1rem center;
}


.tit-message,.tit-invi,.tit-code,.tit-coupon,.tit-sign,
.tit-advise,.tit-about{
    background-color: #fff;
    padding:.3rem;
    margin-top:.3rem;
    padding-left:.8rem;
    background-size:.64rem;
}
.tit-coupon,.tit-about,.tit-sign{
    border-top:.02rem solid #dddddd;
}

.tit-about,.tit-coupon,.tit-sign{
    margin-top:0;
}

.bottom_box{
	overflow: hidden;
	position: fixed;
	bottom:0;
	left:0;
	border-top:1px solid #ddd;
	right:0;
	background:#fff;
}
.bottom_box .index{
	background: url(../images/index.png) no-repeat  center 0.1rem;
	background-size:0.8rem;
}
.bottom_box .index.on{
	background: url(../images/index1.png) no-repeat  center 0.1rem;
	background-size:0.8rem;
}
.bottom_box .choice{
	background: url(../images/choice.png) no-repeat  center 0.1rem;
	background-size:0.8rem;
}
.bottom_box .choice.on{
	background: url(../images/choice1.png) no-repeat  center 0.1rem;
	background-size:0.8rem;
}
.bottom_box  .bespeak{
	background: url(../images/bespeak.png) no-repeat  center 0.1rem;
	background-size:0.8rem;
}
.bottom_box  .bespeak.on{
	background: url(../images/bespeak1.png) no-repeat  center 0.1rem;
	background-size:0.8rem;
}
.bottom_box  .use{
	background: url(../images/use.png) no-repeat  center 0.1rem;
	background-size:0.8rem;
}
.bottom_box  .use.on{
	background: url(../images/use1.png) no-repeat  center 0.1rem;
	background-size:0.8rem;
}
.bottom_box li{
	float: left;
	width:25%;
	text-align: center;
	padding-top:0.93rem;
	padding-bottom:0.1rem;
}
.bottom_box li.on{
	color:#0BDBAC;
}


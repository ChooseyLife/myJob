*{
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}
body{
	background-color: #eee;
}
.main{
	padding-bottom:1.4rem;
}
/*��ҳ*/
.home_page{
	display: block;
}
/*��ѡ*/
.choice_box{
	display: none;
}
/*ԤԼ*/
.bespeak_box{
	display: none;
}
/*�ҵ�*/
.use_box{
	display: none;
}
.bottom_box{
	display: block;
	background: #fff;
	z-index:999;
}
.home_page .title_box{
	line-height: 0.8rem;
}
.home_page .title_box .back{
	display: inline-block;
    height: 100%;
    width: 0.5rem;
    text-align: center;
    line-height: 0.8rem;
}
.head_top .there{
	padding-left:0.74rem;
	background:url(../images/there.png) no-repeat left center;
	background-size: 0.64rem;
	position: absolute;
	left:0;
	top:0;
}
.wares_box{
	overflow: hidden;
	width:100%;
}
.wares_box li{
	
	float: left;
	width:48%;
	margin:1%;
	text-align: center;

}
.wares_box li:nth-child(2n){
	margin-left:0;
}
.wares_box li img{
	width: 100%;
	height:5rem;
	background-color: red;
	border:0;
}

.wares_box .name{
	padding:0.2rem 0  ;
}
.wares_box .price{
	color:#fb8328;
	font-size: 0.34rem;
}
.wares_box .price span{
	font-size: 0.2rem;
	color:#ddd;
	margin-left:0.2rem;
}
.more_btn{
	width:5.2rem;
	height:1.09rem;
	font-size: 0.43rem;
	line-height: 1.09rem;
	margin:0.4rem auto;
	border:1px solid #1bb7ac;
	color:#1bb7ac;
	text-align: center;
	

}
.head{
	position: relative;
	height:6.29rem;
	color:#fff;
}
.swiper-pagination.swiper-pagination-bullets{
	text-align: right;
}
.head_top{
	position: absolute;
	top:0.2rem;
	left:0.2rem;
	right:0.2rem;
	z-index:4;
	text-align: center;
}
.head_top .search{
	background:url(../images/search.png) center;
	background-size: 0.64rem;
	width: 0.64rem;
	height: 0.64rem;
	display: inline-block;
	position: absolute; 
	right:0.8rem;
	top:0; 
}
.head_top .news{
	background:url(../images/news.png) no-repeat center;
	background-size: 0.64rem;
	width: 0.64rem;
	height: 0.64rem;
	display: inline-block;
	position: absolute;
	right:0;
	top:0; 
}
.home_page .nav_box{
	position: absolute;
	top:0;
	left:0;
	right:0;
	height:100%;
	background:red;
	z-index:3;
}
 .hairdressing{
	background: url(../images/hairdressing.png) no-repeat center center;
	background-size: 0.93rem;
}
.project_box .project_readius{
	width: 1.4rem;
	margin: 0 auto;
	background: #f8f8f8;
	border-radius: 20px;
	height: 1.2rem;
}

.project_box .cosmetology{
	background: url(../images/cosmetology.png) no-repeat center center;
	background-size: 0.93rem;
}
.project_box .manicure{
	background: url(../images/manicure.png) no-repeat center center;
	background-size: 0.93rem;
}
.project_box .beauty{
	background: url(../images/Beauty.png) no-repeat center center;
	background-size: 0.93rem;
}
.project_box .dressing{
	background: url(../images/dressing.png) no-repeat center center;
	background-size: 0.93rem;
}
.swiper-container,.swiper-container2{
	height:100%;
}
.swiper-slide{
	background-color: #fff;
}
.swiper-slide img{
	width:100%;
	height: 100%;
}
.store .swiper-slide img{
	height: 5.04rem;
}
.store .swiper-slide p{
	text-align: center;
	padding:0.2rem;
}
.project_box{
	padding:.2rem 0;
	list-style: none;
	overflow: hidden;
	background-color: #fff;
}
.project_box li a{
	display:inline-block;
	padding-top:1rem;
	width: 100%;
	height: 100%;
}
.project_box li:not(.trigon){
	float: left;
	width:20%;
	text-align: center;
	
}
.details ul li:not(.title_box){
	padding-left:0.8rem;
	height:0.8rem;
	line-height: 0.8rem;
	border:1px solid #ddd;
	margin-top:-1px;
}

.trigon{

}
.store_box{
	margin-bottom:0.2rem;
	text-align:center;
}
.Package_box{
	font-size:0;
	background-color: #fff;
	margin-top:.2rem;
}
.Package_box a{
	display: inline-block;
	width:48%;
	margin:1%;
	height:1.5rem;
}
.Package_box .member{
	background: url(../images/huiyuan.png) no-repeat ;
	background-size: 100% 100%;
}
.Package_box .sprog{
	background: url(../images/xinren.png) no-repeat ;
	background-size: 100% 100%;
}
.before{
	position: relative;
	padding-left:0.5rem;
	height:0.8rem;
	line-height: 0.8rem;
	background: #fff;
}
.before:before{
	content: "";
	position: absolute;
	left:0.2rem;
	top:50%;
	border:1px solid #1bb7ac;
	display: inline-block;
	width:0.05rem;
	height:0.15rem;
	background: #1bb7ac;
	transform:translateY(-50%);
}
.store_title{
	font-size:0.49rem;
	color:#000;
	text-align: center;
	background:#eee;
	height:1rem;
	line-height: 1rem;
	position: relative;
	display: inline-block;
}
.store_title:before{
	content: "";
	position: absolute;
	margin-top:-0.07rem;
	top:50%;
	left:-1.8rem;
	background:url(../images/title-left.png) no-repeat center;
	background-size: 1.45rem 0.13rem;
	width: 1.45rem;
	height:0.13rem;
	display: inline-block;
	z-index:9;
}
.store_title:after{
	content: "";
	position: absolute;
	margin-top:-0.07rem;
	top:50%;
	right:-1.8rem;
	background:url(../images/title-right.png) no-repeat center;
	background-size: 1.45rem 0.13rem;
	width: 1.45rem;
	height:0.13rem;
	display: inline-block;
	z-index:9;
}



.store_title span::before{

}
.store_title span::after{
	content:" ";
	background:url(../images/redius.png)no-repeat;
}
.propagate_box1{
	background:#fff;
	margin-bottom:0.2rem;
}
.propagate_img{
	width:100%;
}
.mor{
	color:#1bb7ac;
	float: right;
	vertical-align: middle;
	line-height: 0.8rem;
	margin-right:0.2rem;
}
.hot_box{
	overflow: hidden;
	background: #fff;
    margin-bottom: 0.2rem;
}
.big_hot_box{
	overflow: hidden;
}
.big_hot{
	width:48.5%;
	float: left;
	background: blue;
	margin-left:1%;
	margin-right:1%;
	position: relative;
}
.big_hot p{
	text-align: center;
	line-height: 0.8rem;
	font-size: 0.42rem;
	position: absolute;
	color:#fff;
	bottom:0;
	left:0;
	right:0;
	background-color: rgba(0,0,0,0.2)

}
.popular_box{
	background: #fff;
}
.big_hot img{
	width: 100%;
	height: 100%;
}
.hot_box .big_hot:nth-child(2){
	margin-left:0;
}
.hot{
	overflow: hidden;
	width:98%;
	margin:0.2rem auto ;
}
.hot li:nth-child(1){
	margin-left:0;
}
.hot li{
	float: left;
    width: 15.8%;
    height: 1.3rem;
    background: green;
    margin-left: 1%;
}
.hot li img{
	width:100%;
	height: 100%;
}














































.choice_box .banner{
	height:0.5rem;
	line-height: 0.5rem;
	text-align: center;
	border-bottom:1px solid #ddd;
}
.propagate_title{
	display: inline-block;
}
.choice_box .nav_box{
	overflow: hidden;
}
.choice_box .nav_box li{
	float: left;
	width:50%;
	height:0.5rem;
	border-bottom:2px solid transparent;
	line-height: 0.5rem;
	text-align: center;
}
.choice_box .nav_box li.on{
	color:#fc8f25;
	border-bottom:2px solid #fc8f25;
}
.choice_box .culling{
	overflow: hidden;
}
.store{
	background-color: #fff;
}
.choice_box .culling li{
	float: left;
	height:1rem;
	width:50%;
	background: blue;
}
.choice_box .culling li span{
	display: block;
	width: 100%;
	height:50%;
	background: yellow;
}
.bespeak_box>.title{
	height:0.8rem;
	line-height:0.8rem;
	text-align: center;
	position: relative;
}
.bespeak_box>.title span{
	position: absolute;
	right:0;
	top:0;
	display: inline-block;
	height:0.8rem;
	border-bottom:1px solid #ddd;
}
.bottom_box{
	overflow: hidden;
	position: fixed;
	bottom:0;
	left:0;
	border-top:1px solid #ddd;
	right:0;
}
.bottom_box .index{
	background: url(../images/index.png) no-repeat  center 0.1rem;
	background-size:0.8rem;
}
.bottom_box .index.on{
	background: url(../images/index1.png) no-repeat  center 0.1rem;
	background-size:0.8rem;
}
.bottom_box .index.on a{
	color:#0cc;
}
.bottom_box .choice{
	background: url(../images/choice.png) no-repeat  center 0.1rem;
	background-size:0.8rem;
}
.bottom_box .choice.on{
	background: url(../images/choice1.png) no-repeat  center 0.1rem;
	background-size:0.8rem;
}
.bottom_box  .bespeak{
	background: url(../images/bespeak.png) no-repeat  center 0.1rem;
	background-size:0.8rem;
}
.bottom_box  .bespeak.on{
	background: url(../images/bespeak1.png) no-repeat  center 0.1rem;
	background-size:0.8rem;
}
.bottom_box  .use{
	background: url(../images/use.png) no-repeat  center 0.1rem;
	background-size:0.8rem;
}
.bottom_box  .use.on{
	background: url(../images/use1.png) no-repeat  center 0.1rem;
	background-size:0.8rem;
}
.bottom_box li{
	float: left;
	width:25%;
	text-align: center;
	padding-top:0.93rem;
	padding-bottom:0.1rem;
}
.bottom_box li.on{
	color:#0BDBAC;
}